{
    "version": 2,
    "builds": [
      {
        "src": "job_board_api/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles_build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "job_board_api/wsgi.py"
      }
    ]
  }
  